<tbody class="collapse show collapsing" id="collapse-{{object.uuid}}">
{% for linje in object.linjer.all %}
<tr class="subrow">
    <td colspan="3"></td>
    <td>{{ linje.indberetningstidspunkt }}</td>
    <td>{{ linje.produkttype.fiskeart }}</td>
    <td>{{ linje.kommentar }}</td>
    <td>{{ linje.afgift }}</td>
    <td colspan="2"></td>
</tr>
{% endfor %}
</tbody>
